@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

@prefix baseEntity: <http://www.games.com/entity/>.
@prefix baseProperty: <http://www.games.com/pred/>.
@prefix baseGames: <http://www.games.com/class/>.

baseGames:Id a rdfs:Class.
baseGames:Platform a rdfs:Class.
baseGames:Publisher a rdfs:Class.

baseProperty:name a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range rdfs:Literal.

baseProperty:platform a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range baseGames:Platform.

baseProperty:year a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range rdfs:Literal.

baseProperty:publisher a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range baseGames:Publisher.

baseProperty:na_sales a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range rdfs:Literal.

baseProperty:eu_sales a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range rdfs:Literal.

baseProperty:jp_sales a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range rdfs:Literal.

baseProperty:other_sales a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range rdfs:Literal.

baseProperty:global_sales a rdf:Property;
rdfs:domain baseGames:Id;
rdfs:range rdfs:Literal.

baseProperty:belongs a rdf:Property;
rdfs:domain baseGames:Platform;
rdfs:range baseGames:Publisher.

baseProperty:release a rdf:Property;
rdfs:domain baseGames:Platform;
rdfs:range rdfs:Literal.
